{
  "presets": [
    ["@babel/preset-env", {
      "targets": "defaults, not ie 11, not ie_mob 11",
      "bugfixes": true,
      "corejs": {
        "version": "3.16",
        "proposals": true
      },
      "useBuiltIns": "usage"
    }],
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/proposal-object-rest-spread",
    "@babel/plugin-syntax-bigint"
  ]
}
