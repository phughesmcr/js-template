{
  "presets": [
    ["@babel/preset-env", {
      "targets": "defaults, not ie <= 11",
      "bugfixes": true,
      "shippedProposals": true,
      "corejs": {
        "version": 3,
        "proposals": true
      },
      "useBuiltIns": "usage"
    }],
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties"
  ]
}
