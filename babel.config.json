{
  "env": {
    "esm": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false,
            "targets": {
              "esmodules": true
            },
            "bugfixes": true,
            "corejs": {
              "version": "3.26",
              "proposals": true
            },
            "useBuiltIns": "usage",
            "forceAllTransforms": true
          }
        ]
      ]
    },
    "cjs": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false,
            "targets": {
              "node": "14"
            },
            "bugfixes": true,
            "corejs": {
              "version": "3.26",
              "proposals": true
            },
            "useBuiltIns": "usage",
            "forceAllTransforms": true
          }
        ]
      ]
    },
    "umd": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": "defaults, node >= 10",
            "bugfixes": true,
            "corejs": {
              "version": "3.26",
              "proposals": true
            },
            "useBuiltIns": "usage",
            "forceAllTransforms": true
          }
        ]
      ]
    },
    "iife": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": "defaults",
            "bugfixes": true,
            "corejs": {
              "version": "3.26",
              "proposals": true
            },
            "useBuiltIns": "usage",
            "forceAllTransforms": true
          }
        ]
      ]
    }
  }
}
