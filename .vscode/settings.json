{
  "[typescriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[javascript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "cSpell.words": [
    "caniuse",
    "CODEOWNERS",
    "codepaths",
    "corejs",
    "deno",
    "devtool",
    "dotfiles",
    "downlevel",
    "esmodules",
    "ESNEXT",
    "iife",
    "jsnext",
    "jstemplate",
    "Parens",
    "postbuild",
    "sourcemap",
    "treeshake",
    "tsbuildinfo",
    "tslib",
    "typechecking",
    "typedoc"
  ],
  "editor.rulers": [
    120
  ],
  "deno.enable": true,
  "deno.importMap": "./deno.json",
  "deno.lint": true,
  "editor.formatOnSave": true,
}
